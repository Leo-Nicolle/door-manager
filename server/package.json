{
  "name": "simple-express",
  "version": "1.0.0",
  "description": "express server with babel installed",
  "main": "dist/server.js",
  "scripts": {
    "watch": "rollup -c -w",
    "test": "mocha -r dotenv/config test/*.test.js dotenv_config_path=.env.test --require @babel/register",
    "pretest": "rollup -c && mkdir -p dist/test/db && cp ./config.json dist/test/db/config.json",
    "nodemon": "nodemon -r dotenv/config dist/server.js --watch dist/",
    "start": "npm run watch& npm run nodemon&"
  },
  "keywords": [],
  "author": "latif.s",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esp-ota": "^1.1.3",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "hashdirectory": "^0.1.0",
    "inquirer": "^7.3.3",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "quick-encrypt": "^1.0.8",
    "shelljs": "^0.8.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
