{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "class-methods-use-this": 0,
        "no-useless-constructor": 0,
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-return-assign": 0,
        "array-callback-return": 0
    }
}
